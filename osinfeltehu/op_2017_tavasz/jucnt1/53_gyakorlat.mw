<Group labelreference="L119" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">fibonacci4 := proc(n::integer)
  option remember;
  if n = 0 then 0
  elif n = 1 then 1
  elif n &gt; 0 then fibonacci4(n-1) + fibonacci4(n-2)
  else fibonacci4(n+2) - fibonacci4(n+1)
  fi;
end proc:</Text-field>
