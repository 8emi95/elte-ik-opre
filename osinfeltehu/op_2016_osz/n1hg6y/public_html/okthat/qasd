#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <sys/wait.h>
#include <errno.h>

int main()
{
	int value=2;
	pid_t child=fork();
	if (child <0)
	{
		//error
	}
	else if(child> 0)
	{
		printf("En vagyok a child(kft)\n");
	}
	else
	{
		printf("En vagyok a parent\n");
	}
	return 0;
}