#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>  //fork
#include <sys/wait.h> //waitpid
#include <errno.h> 


int main()
{
 int i;
 pid_t szulo=getpid();
 for (i=0;i<2;i++)
  if (getpid()==szulo) fork(); //forks make a copy of variables
 
 printf("Hello %i %i\n",getpid(),getppid());
 return 0;
}